{"components/common/account-dropdown-signin":"<nav class=\"navBar navBar--sub navBar--account signin-list\">\n    <ul class=\"navBar-section signout-list\">\n      <li>\n        <a class=\"navBar-action signout-list\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}  </a>\n          <span class=\"navUser-or\"> {{ lang 'common.or' }} </span>\n          <a class=\"navBar-action signout-list\" href=\"{{urls.auth.create_account}}\">  {{lang 'common.sign_up'}}</a></li>\n    </ul>\n</nav>\n","components/common/account-dropdown-signout":"<nav class=\"navBar navBar--sub navBar--account signout-list\">\n    <ul class=\"navBar-section signout-list\">\n        {{#if account_page '===' 'orders'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.orders'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.returns_enabled}}\n            {{#if account_page '===' 'returns'}}\n                <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.returns'}}</li>\n            {{else}}\n                <li class=\"navBar-item signout-list\">\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if account_page '===' 'messages'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.show_payment_methods}} \n            {{#if account_page '===' 'payment_methods'}}\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.payment_methods'}}</li>\n            {{else}}\n                <li class=\"navBar-item signout-list\">\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                </li>\n            {{/if}}\n        {{/if}} \n\n        {{#if account_page '===' 'addresses'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.addresses'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n            </li>\n        {{/if}}\n\n        {{#if settings.show_wishlist}}\n            {{#if account_page '===' 'wishlists'}}\n                <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</li>\n            {{else}}\n                <li class=\"navBar-item signout-list\">\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if account_page '===' 'recent_items'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.recently_viewed'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n            </li>\n        {{/if}}\n        {{#if account_page '===' 'settings'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.settings'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n","components/common/account-link":"{{#if settings.account_creation_enabled}}\n<!-- <ul class=\"account-link enabled\"> -->\n    <li class=\"navUser-item navUser-item--account user-icons account\">\n      {{#if customer}}\n      <!-- <a class=\"navUser-action with-logout\" href=\"{{urls.account.index}}\"><svg><use xlink:href=\"#icon-profile\" /></svg></a> -->\n      <!-- <a class=\"navUser-action logout\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a> -->\n      <a class=\"navUser-action account signout-list\" alt=\"User Toolbox\" href=\"javascript:void(0)\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a>\n      <div class=\"dropdown-menu signout-list\" id=\"account-dropdown-signout-list\">\n        <!-- <h2 class=\"container-header\">Account Info</h2> -->\n          {{> components/common/account-dropdown-signout}}\n        <a class=\"navBar-action signout-list\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n        <!-- <hr class=\"left full-width\" /> -->\n        <!-- { {> components / common / currency-selector-dropdown } } -->\n      </div>\n      {{else}}\n        <a class=\"navUser-action account signin-list\" alt=\"User Toolbox\" href=\"{{urls.auth.login}}\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a>\n        <!-- <a class=\"navUser-action login\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> -->\n        <div class=\"dropdown-menu signin-list\" id=\"account-dropdown-signin-list\">\n          <!-- <h2 class=\"container-header\">Account Info</h2> -->\n            {{> components/common/account-dropdown-signin}}\n            <!-- <hr class=\"left full-width\" /> -->\n            <!-- { {> components / common / currency-selector-dropdown } } -->\n            <!-- <a class=\"button button--primary signin-register logout-button\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> -->\n        </div>\n      {{/if}}\n     </li>\n{{/if}}\n<!-- </ul> -->\n","components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<li class=\"navUser-item currency-menu\">\n    <a class=\"navUser-action has-dropdown\" alt=\"Currency Selector\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{currency_selector.active_currency_code}} <i class=\"icon currency\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n        {{#each currency_selector.currencies}}\n        <li class=\"dropdown-menu-item\">\n              <a class=\"currency-name-switcher\" href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                <strong>{{{flag}}} {{name}}</strong>\n                {{else}}\n                {{{flag}}} {{name}}\n                {{/if}}\n            </a>\n        </li>\n        {{/each}}\n    </ul>\n</li>\n{{/if}}\n","components/common/header":"{{{region name=\"header_top--global\"}}}\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n<header class=\"header header-logo--{{theme_settings.logo-position}}\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n    </a>\r\n\r\n  <div class=\"logo-user-section container\">\r\n    {{#if page_type '===' 'default'}}\r\n       <!-- <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n            {{!> components/common/store-logo}}\r\n        </h1>-->\r\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n           <a class=\"logo\" href=\"/\" title=\"Signal Sounds\" aria-label=\"store logo\">\r\n                <img src=\"https://signal-sounds-alb.mybigcommerce.com/content/header-logo.svg\" width=\"164\" height=\"75\" alt=\"Signal Sounds\" />\r\n           </a>\r\n        </div>\r\n        {{> components/common/navigation}}\r\n    {{else}}\r\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n            {{!> components/common/store-logo}}\r\n            <a class=\"logo\" href=\"/\" title=\"Signal Sounds\" aria-label=\"store logo\">\r\n                <img src=\"https://signal-sounds-alb.mybigcommerce.com/content/header-logo.svg\" width=\"164\" height=\"75\" alt=\"Signal Sounds\" />\r\n           </a>\r\n        </div>\r\n        {{> components/common/navigation}}\r\n    {{/if}}\r\n  </div>\r\n    <div id=\"menu-wrapper\">\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n          {{> components/common/navigation-menu}}\r\n          <div class=\"clear\"></div>\r\n        </div>\r\n      <div class=\"clear\"></div>\r\n    </div>\r\n    <div class=\"clear\"></div>\r\n\r\n</header>\r\n<div class=\"below_nav_container\">\r\n    {{{region name=\"header_below_nav--global\"}}}\r\n</div>\r\n<div class=\"below_nav_clear clear\" style=\"margin-top: 0px;\"></div>\r\n","components/common/mega-nav-page-list":"<div class=\"nav-page-list mega-nav-page-list\">\n    {{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root second-level-parent topLevel sideNavTopLevel\" href=\"{{url}}\">\n        {{name}} <i class=\"icon mega-nav-pages\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n        <i class=\"icon navPages-action-moreIcon fixed-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n    </a>\n    <div class=\"navPage-subMenu navPage-subMenu-horizontal second-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n        <ul class=\"navPage-subMenu-list\">\n    \n            {{#each children}}\n                <li class=\"navPage-subMenu-item-child second-level\">\n                    {{#if children}}\n                        <a\n                            class=\"navPage-subMenu-action navPages-action has-subMenu second-level\"\n                            href=\"{{url}}\">\n                            {{name}} <i class=\"icon mega-nav-pages\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                        </a>\n                        <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                            {{#each children}}\n                            <li class=\"navPage-childList-item\">\n                                <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}} </a>\n                            </li>\n                            {{/each}}\n                        </ul>\n                    {{else}}\n                        <a class=\"navPage-subMenu-action navPages-action second-level\" href=\"{{url}}\">{{name}}</a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n    {{else}}\n    <a class=\"navPages-action sideNavTopLevel\" href=\"{{url}}\">{{name}}</a>\n    {{/if}}\n    <div class=\"clear\"></div>\n    </div>\n    ","components/common/navigation":"<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item navUser-item--storecredit desktop\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\">\n                        <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n                <div class=\"clear\"></div>\n            </li>\n        {{/if}}\n\n        <div class=\"dropdown dropdown--quickSearch desktop\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n            {{> components/common/quick-search}}\n        </div>\n\n        <div class=\"user-icons\">\n          <!-- <li class=\"navUser-item navUser-item--cart user-icons\">\n              <a\n                  class=\"navUser-action\"\n                  data-cart-preview\n                  data-dropdown=\"cart-preview-dropdown\"\n                  data-options=\"align:right\"\n                  href=\"{{urls.cart}}\"\n                  aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n                  <span class=\"navUser-item-cartLabel\"><svg class=\"header-icon cart-icon\"><use xlink:href=\"#icon-cart\"></use></svg></span> <span class=\"countPill cart-quantity\"></span>\n              </a>\n\n              <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n          </li> -->\n\n          <li class=\"navUser-item navUser-item--cart user-icons\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <!-- <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span> -->\n                <span class=\"navUser-item-cartLabel\"><svg class=\"header-icon cart-icon\"><use xlink:href=\"#icon-cart\"></use></svg></span> <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n\n          {{>components/common/currency-selector}}\n\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item user-icons gift-certificates\">\n                <a alt=\"Gift Certificates\" class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\"><svg class=\"header-icon gift-icon\"><use xlink:href=\"#icon-gift\" /></svg></a>\n            </li>\n        {{/if}}\n\n        <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav><svg class=\"header-icon compare-icon\"><use xlink:href=\"#icon-compare\"></use></svg><span class=\"countPill countPill--positive countPill--alt compare\"></span></a>\n\n        <!-- <li class=\"navUser-item navUser-item--account user-icons account\"> -->\n                {{#if settings.account_creation_enabled}}\n                    <!-- <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"><svg class=\"header-icon account-icon\"><use xlink:href=\"#icon-account\"></use></svg></a> -->\n                    {{> components/common/account-link}}\n                {{/if}}\n        <!-- </li> -->\n\n        <li class=\"navUser-item quicksearch mobile\">\n            <a class=\"navUser-action navUser-action--quickSearch desktop-toggle\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><svg class=\"header-icon search-icon\"><use xlink:href=\"#icon-search\"></use></svg></a>\n        </li>\n\n    </ul>\n    </div>\n\n</nav>\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal second-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n  <div class=\"category-title\">\n    <h2><a class=\"category-title\" href=\"{{url}}\">{{name}}</a></h2>\n    <div class=\"flyout-pagination-container top\">\n      <a class=\"navPages-action previous\">\n        <i class=\"icon prev\" aria-hidden=\"true\">\n          <svg><use xlink:href=\"#icon-circle-left\" /></svg>\n        </i>\n      </a>\n      <!-- <span class=\"dots\"></span> -->\n      <a class=\"navPages-action next\">\n        <i class=\"icon next\" aria-hidden=\"true\">\n          <svg><use xlink:href=\"#icon-circle-right\" /></svg>\n        </i>\n      </a>\n    </div>\n  </div>\n  <div class=\"flyout-pagination-container bottom\">\n    <a class=\"navPages-action previous\">\n      <i class=\"icon prev\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n      </i>\n    </a>\n    <span class=\"dots\"></span>\n    <a class=\"navPages-action next\">\n      <i class=\"icon next\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n      </i>\n    </a>\n  </div>\n\n  <div class=\"desktop\">\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child second-level\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon second-level\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown-third-level}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">{{name}}</a>\n                    <!-- <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a> -->\n                {{/if}}\n            </li>\n        {{/each}}\n        <!-- <div class=\"flyout-pagination-container bottom\">\n          <a class=\"navPages-action previous\">\n            <i class=\"icon prev\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n          <span class=\"dots\"></span>\n          <a class=\"navPages-action next\">\n            <i class=\"icon next\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n        </div> -->\n\n    </ul>\n    </div>\n\n<div class=\"mobile\">\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child second-level\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">\n                        {{name}}\n                    </a>\n                    <i class=\"icon navPages-action-moreIcon second-level\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    {{> components/common/navigation-dropdown-third-level}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">{{name}}</a>\n                    <!-- <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a> -->\n                {{/if}}\n            </li>\n        {{/each}}\n        <!-- <div class=\"flyout-pagination-container bottom\">\n          <a class=\"navPages-action previous\">\n            <i class=\"icon prev\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n          <span class=\"dots\"></span>\n          <a class=\"navPages-action next\">\n            <i class=\"icon next\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n        </div> -->\n\n    </ul>\n    </div>\n</div>\n","components/common/navigation-dropdown-fourth-level-plus":"<div class=\"navPage-subMenu navPage-subMenu-horizontal fourth-level-plus-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown-fourth-level-plus}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n          