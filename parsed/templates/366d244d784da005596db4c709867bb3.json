{"components/common/navigation-simple-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal second-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n  <div class=\"category-title\">\n    <h2><a class=\"category-title\" href=\"{{url}}\">{{name}}</a></h2>\n    <div class=\"flyout-pagination-container top\">\n      <a class=\"navPages-action previous\">\n        <i class=\"icon prev\" aria-hidden=\"true\">\n          <svg><use xlink:href=\"#icon-circle-left\" /></svg>\n        </i>\n      </a>\n      <!-- <span class=\"dots\"></span> -->\n      <a class=\"navPages-action next\">\n        <i class=\"icon next\" aria-hidden=\"true\">\n          <svg><use xlink:href=\"#icon-circle-right\" /></svg>\n        </i>\n      </a>\n    </div>\n  </div>\n  <div class=\"flyout-pagination-container bottom\">\n    <a class=\"navPages-action previous\">\n      <i class=\"icon prev\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n      </i>\n    </a>\n    <span class=\"dots\"></span>\n    <a class=\"navPages-action next\">\n      <i class=\"icon next\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n      </i>\n    </a>\n  </div>\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child second-level\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon second-level\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-simple-dropdown-third-level}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} second-level\" href=\"{{url}}\">{{name}}</a>\n                    <!-- <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a> -->\n                {{/if}}\n            </li>\n        {{/each}}\n        <!-- <div class=\"flyout-pagination-container bottom\">\n          <a class=\"navPages-action previous\">\n            <i class=\"icon prev\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n          <span class=\"dots\"></span>\n          <a class=\"navPages-action next\">\n            <i class=\"icon next\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n        </div> -->\n\n    </ul>\n</div>\n","components/common/navigation-simple-dropdown-third-level":"<div class=\"navPage-subMenu navPage-subMenu-horizontal third-level-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child third-level\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\n                        {{name}}\n                    </a>\n                    \n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n        <li class=\"navPage-subMenu-item-paren view-all\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a>\n        </li>\n    </ul>\n</div>\n"}
