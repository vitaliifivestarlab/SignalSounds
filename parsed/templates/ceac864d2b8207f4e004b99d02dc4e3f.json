{"components/blog/post-page":"<article class=\"blog\">\n    <div class=\"blog-post-figure\">\n      {{#if post.thumbnail}}\n          <figure class=\"blog-thumbnail blog-image uploaded\">\n              <a href=\"{{post.url}}\">\n                  <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\n              </a>\n          </figure>\n          {{else}}\n            <div class=\"blog-image placeholder-image\"><br /></div>\n      {{/if}}\n    </div>\n\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            <h2 class=\"blog-title\">\n                <a href=\"{{post.url}}\" data-maxlength=\"24\">{{post.title}}</a>\n            </h2>\n            <p class=\"blog-date\" data-maxlength=\"55\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\n        </header>\n\n        <div class=\"blog-post\">\n            {{#if post.body}}\n                {{{post.body}}}\n            {{else}}\n                {{{post.summary}}}\n                {{#if post.show_read_more}}\n                    &hellip; <a class=\"read-more\" href=\"{{url}}\">read more</a>\n                {{/if}}\n            {{/if}}\n        </div>\n\n\n        {{#if show_tags}}\n            {{#if post.tags}}\n                <ul class=\"tags\">\n                    {{#each post.tags}}\n                    <li class=\"tag\">\n                        <a href=\"{{url}}\">#{{name}}</a>\n                    </li>\n                    {{/each}}\n                </ul>\n            {{/if}}\n        {{/if}}\n\n        {{>components/common/share url=post.url}}\n\n    </div>\n</article>\n","components/common/share":"{{#if settings.add_this.buttons}}\n    {{assignVar 'encodedUrl' (encodeURI url)}}\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\n    <div>\n        <ul class=\"socialLinks\">\n            {{#each settings.add_this.buttons}}\n                <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                    {{#if service '===' 'facebook'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\"> <g> <path fill=\"none\" d=\"M0 0h24v24H0z\"/> <path d=\"M14 13.5h2.5l1-4H14v-2c0-1.03 0-2 2-2h1.5V2.14c-.326-.043-1.557-.14-2.857-.14C11.928 2 10 3.657 10 6.7v2.8H7v4h3V22h4v-8.5z\"/> </g></svg>\n                    </a>\n                    {{else if service '===' 'email'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                       target=\"_self\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg version=\"1.1\" id=\"Capa_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\" viewBox=\"0 0 490 490\" style=\"enable-background:new 0 0 490 490;\" xml:space=\"preserve\"><g><g><g><polygon points=\"323.6,245 490,389.3 490,100.7 \"/><path d=\"M308.8,257.8l-57.5,49.8c-3.5,3.1-8.8,3.1-12.4,0l-57.7-49.8L17.1,400.2h455.4L308.8,257.8z\"/><polygon points=\"0,100.7 0,389.3 166.4,245 \"/><polygon points=\"472.9