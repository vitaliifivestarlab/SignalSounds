ories}}\n          <li class=\"navPages-item top-level categories{{#if is_active}} activePage{{/if}}\">\n          {{#if ../theme_settings.navigation_design '===' 'simple'}}\n              {{> components/common/navigation-list}}\n          {{/if}}\n          {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n              {{> components/common/navigation-list-alternate}}\n          {{/if}}\n          </li>\n        {{/each}}\n        </div>\n      <div class=\"pagination-container\"><a class=\"navPages-action previous\"><i class=\"icon prev\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-left\" /></svg></i></a><span class=\"dots\"></span><a class=\"navPages-action next\"><i class=\"icon next\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-left\" /></svg></i></a></div>\n      <hr class=\"sidenav\"/>\n        </div>\n        </div>\n\n        {{#if theme_settings.show_pages_in_mobile_nav '===' true}}\n        <div class=\"sticky-navpages\">\n          <h2 class=\"container-header mobile\">Navigation</h2>\n          {{#each pages}}\n            <li class=\"navPages-item top-level{{#if is_active}} activePage{{/if}} pages mega-nav-pages\">\n                {{> components/common/mega-nav-page-list}}\n            </li>\n          {{/each}}\n        </div>\n        {{/if}}\n\n        <!-- BRANDS VISIBILITY -->\n        <div class=\"brands {{#if theme_settings.show_brands_in_main_mega_nav '===' false}}brands-hidden {{/if}}{{#if theme_settings.show_brands_in_sticky_nav '===' false}}sticky-brands-hidden{{/if}}\">\n          <h2 class=\"container-header mobile\">Brands</h2>\n            {{#each shop_by_brand}}\n                   <li class=\"navPages-item mobile\">\n                        <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                    </li>\n            {{/each}}\n                    <li class=\"navPages-item mobile view-all brands\">\n                        <a class=\"navPages-action\" href=\"/brands/\">View all Brands</a>\n                    </li>\n        </div>\n        {{#if customer.store_credit.value '>' 0}}\n        <ul class=\"nav-user-menu\">\n            <li class=\"navUser-item navUser-item--storecredit\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\">\n                        <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n                <div class=\"clear\"></div>\n            </li>\n        </ul>\n        {{/if}}\n            <div class=\"clear\"></div>\n\n        <div class=\"clear\"></div>\n      </div>\n      <div class=\"clear\"></div>\n    </div>\n    <div class=\"nav-right-fixed\">\n    <div class=\"nav-right\">\n    <ul class=\"navPages-list nav-page-list one {{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n      {{#unless theme_settings.hide_category_navigation}}\n        {{#each categories}}\n            <li class=\"navPages-item categories\">\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                {{> components/common/navigation-list}}\n            {{/if}}\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                {{> components/common/navigation-list-alternate}}\n            {{/if}}\n            </li>\n        {{/each}}\n        {{/unless}}\n        {{#unless theme_settings.hide_content_navigation}}\n        <ul class=\"navPages-list nav-page-list two\">\n       {{#each pages}}\n           <li class=\"navPages-item\">\n     