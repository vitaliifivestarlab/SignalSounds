{"components/products/description-accordion-tabs":"<div class=\"wrapper\">\r\n  <div class=\"tabs\">\r\n    <div class=\"tab\">\r\n  {{#if product.description}}\r\n  {{#if theme_settings.show_product_overview_tab '===' true}}\r\n      <input type=\"radio\" name=\"css-tabs\" id=\"tab-1\" checked class=\"tab-switch\">\r\n      <label for=\"tab-1\" class=\"tab-label tab-label1\">Product Overview</label>\r\n      <div class=\"tab-content\" id=\"product-tab\">\r\n        {{{product.description}}}\r\n      </div>\r\n  {{/if}}\r\n  {{/if}}\r\n  </div>\r\n\r\n    <div class=\"tab\">\r\n      <input type=\"radio\" name=\"css-tabs\" id=\"tab-2\" class=\"tab-switch\">\r\n      <label for=\"tab-2\" class=\"tab-label tab-label2\">Technical Spec</label>\r\n      <div class=\"tab-content\" id=\"product-tab2\">\r\n             {{#each product.custom_fields}}\r\n    {{#if name}}\r\n      {{#if name '===' 'Technical Spec'}}\r\n        {{{value}}}\r\n      {{/if}}\r\n    {{/if}}\r\n  {{/each}} \r\n            </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n\r\n  "}
